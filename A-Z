.MODEL SMALL 
.STACK 100H 

.CODE 
MAIN PROC 
MOV CX, 
26 MOV DL, 65 

PRINT_UPPER: 
MOV AH, 02H 
INT 21H 
    
INC DL 
LOOP PRINT_UPPER 

MOV DL, ' ' 
MOV AH, 02H 
INT 21H 

MOV CX, 26 
MOV DL, 97 

PRINT_LOWER: 
MOV AH, 02H 
INT 21H 

INC DL 
LOOP PRINT_LOWER 

MOV AH, 4CH 
INT 21H 
MAIN ENDP 
END MAIN
